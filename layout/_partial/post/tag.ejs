<% if (item.tags && item.tags.length){ %>
  <%
  var tags = [];
  item.tags.forEach(function(tag){
    tags.push('<a class="am-badge am-badge-primary" href="' + config.root + tag.path + '">' + tag.name + '</a>');
  });
  %>
  <div class="tags">
    <i class="am-icon-tag"></i> 标签:  <%- tags.join('&nbsp;') %>
  </div>
<% } %>
